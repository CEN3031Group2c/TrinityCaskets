    "concurrently": "^4.1.0",
    "config": "^3.2.4",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mocha": "^6.2.1",
    "mongoose": "^5.7.1",
    "morgan": "^1.9.1",
    "prop-types": "^15.7.2",
    "redux": "^4.0.4",
